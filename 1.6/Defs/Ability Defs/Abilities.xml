  <!--====================== Void Breath ======================-->
  
  <VEF.Abilities.AbilityDef>
        <defName>Projectile_Void_Breath</defName>
        <label>Void breath</label>
        <description>Rawr</description>
        <abilityClass>DD.Ability_ShootProjectile_Dragon</abilityClass>
        <targetMode>Pawn</targetMode>
        <castTime>10</castTime>
        <range>54</range>
        <chance>.4</chance>
        <radius>5</radius>
        <cooldownTime>90</cooldownTime>
